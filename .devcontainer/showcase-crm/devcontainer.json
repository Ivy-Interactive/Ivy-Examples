{
    "name": "Showcase CRM",
    "image": "mcr.microsoft.com/dotnet/sdk:10.0",
    "features": {},
    "workspaceFolder": "/workspaces/Ivy-Examples/project-demos/showcase-crm",
    "postCreateCommand": "dotnet tool install -g Ivy.Console",
    "postStartCommand": "dotnet watch",
    "forwardPorts": [5010],
    "portsAttributes": {
      "5010": {
        "label": "Showcase CRM",
        "onAutoForward": "openPreview"
      }
    },
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-dotnettools.csharp",
          "ms-vscode.powershell",
          "ms-dotnettools.csdevkit"
        ]
      },
      "codespaces": {
        "openFiles": ["Apps/DashboardApp.cs"]
      }
    }
  }