<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <NoWarn>CS8618;CS8603;CS8602;CS8604;CS9113</NoWarn>
    <RootNamespace>MimeMappingExample</RootNamespace>
    <UserSecretsId>eb410f4a-856d-441d-859e-ee109490ab72</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <EmbeddedResource Include="Assets/**/*" />
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="Ivy" Version="1.2.6" />
    <PackageReference Include="MimeMapping" Version="3.1.0" />
  </ItemGroup>
  
</Project>
