<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net10.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<NoWarn>CS8618;CS8603;CS8602;CS8604;CS9113</NoWarn>
		<RootNamespace>RestSharpExample</RootNamespace>
	  <UserSecretsId>876a667d-cf0d-4ed3-a195-380bc365ad51</UserSecretsId>
  </PropertyGroup>

	<ItemGroup>
		<EmbeddedResource Include="Assets/**/*" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Ivy" Version="1.2.15" />
		<PackageReference Include="RestSharp" Version="113.0.0" />
	</ItemGroup>

	<ItemGroup>
		<Folder Include="Apps" />
		<Folder Include="Connections" />
	</ItemGroup>

</Project>
