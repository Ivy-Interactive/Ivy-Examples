<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <NoWarn>CS8618;CS8603;CS8602;CS8604;CS9113;SKEXP0001;SKEXP0010;SKEXP0070</NoWarn>
    <RootNamespace>MicrosoftAgentFramework</RootNamespace>
    <UserSecretsId>55bdd9dd-700b-4bfb-bbac-4cddf39cecc7</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <EmbeddedResource Include="Assets/**/*" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Ivy" Version="1.2.15" />
    <PackageReference Include="Microsoft.Agents.AI" Version="1.0.0-preview.251204.1" />
    <PackageReference Include="OllamaSharp" Version="5.4.12" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Apps" />
    <Folder Include="Views" />
    <Folder Include="Tools" />
    <Folder Include="Models" />
    <Folder Include="Services" />
  </ItemGroup>

</Project>

