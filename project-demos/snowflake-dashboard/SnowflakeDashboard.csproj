<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <NoWarn>CS8618;CS8603;CS8602;CS8604;CS9113</NoWarn>
    <RootNamespace>SnowflakeDashboard</RootNamespace>
    <UserSecretsId>eaa0f587-8bd7-4165-8bb4-0e62dad3c0a7</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <EmbeddedResource Include="Assets/**/*" />
    <EmbeddedResource Include="Apps/**/*.cs" />
  </ItemGroup>

  
    <ItemGroup>
      <PackageReference Include="Ivy" Version="1.1.1.0" />
      <PackageReference Include="Snowflake.Data" Version="5.1.0" />
    </ItemGroup>
    

  <ItemGroup>
    <Folder Include="Apps" />
    <Folder Include="Connections" />
  </ItemGroup>

</Project>
